{
    "easycom": {
        "autoscan": true,
        "custom": {
            "^s-(.*)": "@/sheep/components/s-$1/s-$1.vue",
            "^su-(.*)": "@/sheep/ui/su-$1/su-$1.vue"
        }
    },
    "pages": [
        {
            "path": "pages/login/index",
            "style": {
                "navigationBarTitleText": "角色选择"
            },
            "meta": {
                "auth": false,
                "sync": true,
                "title": "角色选择",
                "group": "登录"
            }
        },
        {
            "path": "pages/login/login",
            "style": {
                "navigationBarTitleText": "登录"
            },
            "meta": {
                "auth": false,
                "sync": true,
                "title": "登录",
                "group": "登录"
            }
        },
        {
            "path": "pages/login/get-code",
            "style": {
                "navigationBarTitleText": "获取验证码"
            },
            "meta": {
                "auth": false,
                "sync": true,
                "title": "获取验证码",
                "group": "登录"
            }
        },
        {
            "path": "pages/index/index",
            "aliasPath": "/",
            "style": {
                "navigationBarTitleText": "首页",
                "enablePullDownRefresh": true
            },
            "meta": {
                "auth": false,
                "sync": true,
                "title": "首页",
                "group": "首页"
            }
        }
    ],
    "subPackages": [
        {
            "root": "pages/house",
            "pages": [
                {
                    "path": "list",
                    "style": {
                        "navigationBarTitleText": "房源"
                    },
                    "meta": {
                        "sync": true,
                        "title": "房源列表",
                        "group": "房源"
                    }
                },
                {
                    "path": "add",
                    "style": {
                        "navigationBarTitleText": "录入房源"
                    },
                    "meta": {
                        "sync": true,
                        "title": "录入房源",
                        "group": "房源"
                    }
                },
                {
                    "path": "select",
                    "style": {
                        "navigationBarTitleText": "选择房源",
                        "navigationStyle": "custom"
                    },
                    "meta": {
                        "sync": true,
                        "title": "选择房源",
                        "group": "房源"
                    }
                },
                {
                    "path": "detail",
                    "style": {
                        "navigationBarTitleText": "房源详情"
                    },
                    "meta": {
                        "sync": true,
                        "title": "房源详情",
                        "group": "房源"
                    }
                }
            ]
        }
    ],
    "globalStyle": {
        "navigationBarTextStyle": "black",
        "navigationBarTitleText": "芋道商城",
        "navigationBarBackgroundColor": "#FFFFFF",
        "backgroundColor": "#FFFFFF",
        "navigationStyle": "custom"
    },
    "tabBar": {
        "list": [
            {
                "pagePath": "pages/index/index"
            },
            {
                "pagePath": "pages/index/category"
            },
            {
                "pagePath": "pages/index/cart"
            },
            {
                "pagePath": "pages/index/user"
            }
        ]
    }
}
